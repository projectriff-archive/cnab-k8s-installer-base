{
    "name": "kab",
    "version": "0.1.0",
    "description": "A short description of your bundle",
    "keywords": [
        "cnab",
        "kab"
    ],
    "maintainers": [
        {
            "name": "Swapnil Bawaskar",
            "email": "sbawaskar@pivotal.io",
            "url": "https://example.com"
        }
    ],
    "invocationImages": {
        "cnab": {
            "name": "cnab",
            "builder": "docker",
            "configuration": {
                "registry": "sbawaska"
            }
        }
    },
    "parameters": {
        "target_registry": {
            "type": "string",
            "metadata": {
                "description": "URL of the registry where the images should be pushed to e.g: localhost:5000/testuser"
            },
            "destination": {
                "env": "TARGET_REGISTRY"
            }
        },
        "node_port": {
            "type": "boolean",
            "destination": {
                "env": "NODE_PORT"
            },
            "default": "false"
        }
    },
    "credentials": null
}
